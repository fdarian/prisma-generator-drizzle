		"lodash": "^4.17.21",
		"pluralize": "^8.0.0"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.202",
		"@types/node": "^20.11.5",
		"@types/pluralize": "^0.0.33",
		"@types/prettier": "3.0.0",
		"prisma": "5.8.1",
		"tsup": "^8.0.1",
		"typescript": "5.3.3"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/farreldarian/prisma-generator-drizzle"
